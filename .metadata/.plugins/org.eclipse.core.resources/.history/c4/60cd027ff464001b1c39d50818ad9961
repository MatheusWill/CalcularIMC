package oliveira.willian.matheus.model;


public class Calculo {
	
	private String resultadoImc;
	private double resultadoNcd;
	
	Pessoa p = new Pessoa();
	
	public void imc() {
		this.resultadoImc = p.getAltura();
	}
	//public double getResultadoImc() {
		//return this.resultadoImc;
	}
	public void ncd() {
		this.resultadoNcd = p.getPeso();
	}
	public double getResultadoNcd() {
		return this.resultadoNcd;
	}
	
}

