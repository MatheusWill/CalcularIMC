package oliveira.willian.matheus.model;


public class Calculo {
	
	private double imc;
	private double ncd;
	
	Pessoa p = new Pessoa();
	
	public void imc() {
		this.imc = p.getAltura();
	}
	public double getImc() {
		return this.imc;
	}
	public void ncd() {
		this.ncd = p.getPeso();
	}
	public double getNcd() {
		return this.ncd;
	}
	
}

